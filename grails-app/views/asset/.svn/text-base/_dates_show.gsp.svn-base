			<td valign="top" class="name"><g:message code="asset.removedFromInventoryDate.label" default="Removed From Inventory Date" /></td>
			<td valign="top" class="value"><g:formatDate date="${assetInstance?.removedFromInventoryDate}" /></td>
		</tr>
		
		<tr class="prop">
			<td valign="top" class="name"><g:message code="asset.replacementAvailabilityDate.label" default="Replacement Availability Date" /></td>
			<td valign="top" class="value"><g:formatDate date="${assetInstance?.replacementAvailabilityDate}" /></td>
			<td valign="top" class="name"><g:message code="asset.migrationCompletionDate.label" default="Migration Completion Date" /></td>
