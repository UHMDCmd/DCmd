<table>
	<tbody>
		<tr class="prop">
			<td valign="top" class="name"><g:message code="asset.endOfServiceLife.label" default="End Of Service Life" /></td>
			<td valign="top" class="value"><g:formatDate date="${assetInstance?.endOfServiceLife}" /></td>
			<td valign="top" class="name"><g:message code="asset.warrantyEndDate.label" default="Warranty End Date" /></td>
			<td valign="top" class="value"><g:formatDate date="${assetInstance?.warrantyEndDate}" /></td>
		</tr>
		
		<tr class="prop">
			<td valign="top" class="name"><g:message code="asset.retiredDate.label" default="Retired Date" /></td>
